{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentUserName",
      "WithMachineName",
      "WithThreadId",
      "WithSpan",
      "WithExceptionDetails",
      "WithDemystifiedStackTraces"
    ]
  },
  "ApiOptions": {
    "Port": 443,
    "AllowedOrigins": [
      "https://blogs.sparkify.dev",
      "blogs.sparkify.dev"
    ]
  },
  "DatabaseOptions": {
    "Name": "Sparkify_v_0_1",
    "Http": "http://127.0.0.1:8888",
    "TcpHostName": "127.0.0.1",
    "TcpPort": "38888"
  },
  "OtlpOptions": {
    "SinkEndpoint": "http://127.0.0.1:4317"
  }
}
