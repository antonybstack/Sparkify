              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PayloadOfIEnumerableOfIndexEntity": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "data": {
            "type": "array",
            "nullable": true,
            "items": {
              "$ref": "#/components/schemas/IndexEntity"
            }
          },
          "stats": {
            "$ref": "#/components/schemas/RequestStatistics"
          }
        }
      },
      "IndexEntity": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          },
          "blogId": {
            "type": "string"
          },
          "link": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "content": {
            "type": "string",
            "nullable": true
          },
          "subtitle": {
            "type": "string",
            "nullable": true
          },
          "logoName": {
            "type": "string"
          },
          "authors": {
            "type": "array",
            "nullable": true,
            "items": {
              "type": "string"
            }
          },
          "categories": {
            "type": "array",
            "nullable": true,
            "items": {
              "type": "string"
            }
          },
          "date": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "RequestStatistics": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "durationInMs": {
            "type": "integer",
            "format": "int64"
          },
          "totalResults": {
            "type": "integer",
            "format": "int64"
          }
        }
      }
    }
  }
}
