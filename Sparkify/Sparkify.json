          "404": {
            "description": ""
          }
        }
      }
    },
    "/api/blog": {
      "post": {
        "operationId": "PostApiBlog",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "object",
                "properties": {
                  "requestBody": {
                    "type": "string"
                  }
                },
                "required": [
                  "requestBody"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
