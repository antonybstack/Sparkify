{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Clusters": {
      "apiCluster": {
        "Destinations": {
          "Api": {
            "Address": "http://localhost:6002"
          }
        }
      }
    }
  }
}